#include <stdio.h>

/**
 * print_to_98 - Prints numbers from a given number to 98
 *
 * @n: int starting point
 *
 * Return: nothing
 */

void print_to_98(int n)
{
	int i;

	if (n >= 0 && n <= 98)
	{
		for (i = n; i <= 98; i++)
		{
			printf("%d", i);
			if(n == 98)
			{
				putchar('\n');
			}
			else
			{
				printf(", ");
			}
		}
	}
	else if (n > 98)
	{
		for (i = n; i >= 98; i--)
		{
			printf("%d", i);
			if(n == 98)
			{
				putchar('\n');
			}
			else
			{
				printf(", ");
			}
		}
	}
	else if (n < 0)
	{
		for (i = n; i <= 98; i++)
		{
			printf("%d", i);
			if(n == 98)
			{
				putchar('\n');
			}
			else
			{
				printf(", ");
			}
		}
	}
}
